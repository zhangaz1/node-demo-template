    "name": "node-demo-template",
    "version": "0.0.0",
    "description": "node demo template",
    "main": "index.js",
    "scripts": {
        "start": "nodemon",
        "start:back": "nodemon --watch './src/index.ts' --ignore './src/*.spec.ts' --exec ts-node ./src/index.ts",
        "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
        "watch:serve": "ts-node ./src/index.ts",
